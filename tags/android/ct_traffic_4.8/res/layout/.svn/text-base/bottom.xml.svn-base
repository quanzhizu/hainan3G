    android:baselineAligned="false"
    android:orientation="horizontal" >

    <ImageButton
        android:id="@+id/id_bottom_car_park"
        style="@style/bottom"
        android:src="@drawable/car_park" />

    <ImageButton
        android:id="@+id/id_bus_routes"
        style="@style/bottom"
        android:src="@drawable/bus_routes" />

    <ImageButton
        android:id="@+id/id_bottom_traffic"
        style="@style/bottom"
        android:src="@drawable/traffic" />

    <ImageButton
        android:id="@+id/id_bottom_search"
        style="@style/bottom"
        android:src="@drawable/search_normal" />

    <ImageButton
        android:id="@+id/id_bottom_hotspot"
        style="@style/bottom"
        android:src="@drawable/hotspot" />

</LinearLayout>