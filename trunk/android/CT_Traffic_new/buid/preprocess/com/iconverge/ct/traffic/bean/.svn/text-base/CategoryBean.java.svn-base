package com.iconverge.ct.traffic.bean;

public class CategoryBean {

	private long id;
	private String name;
	private long pId;

	public CategoryBean() {
		super();
		// TODO Auto-generated constructor stub
	}

	public CategoryBean(long id, String name, long pId) {
		super();
		this.id = id;
		this.name = name;
		this.pId = pId;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public long getpId() {
		return pId;
	}

	public void setpId(long pId) {
		this.pId = pId;
	}

	@Override
	public int hashCode() {
		// TODO Auto-generated method stub
		return String.valueOf(id).hashCode();
	}

	@Override
	public boolean equals(Object o) {
		if (o instanceof CategoryBean) {
			if (id != -1) {
				return id == ((CategoryBean)o).id;
			}
		}
		return false;
	}

}
