<?xml version="1.0" encoding="UTF-8"?>
<selector
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:constantSize="true">
	<item
		android:state_checked="true"
		android:drawable="@drawable/ct_traffic__checkbox_checked" /> <!-- pressed -->
	<item
		android:state_enabled="false"
		android:drawable="@drawable/ct_traffic__checkbox_unchecked" /> <!-- disabled -->
	<item
		android:state_enabled="true"
		android:drawable="@drawable/ct_traffic__checkbox_unchecked" /> <!-- default -->

</selector>