<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <!-- <item name="android:background">@drawable/bg_launch</item> -->
    </style>

    <style name="AppTheme.ProgressBar.Small" parent="@android:style/Widget.ProgressBar.Small">
        <!-- <item name="android:background">@drawable/bg_launch</item> -->
    </style>

    <style name="AppTheme.ProgressBar" parent="@style/Widget.Sherlock.Light.ProgressBar">
        <!-- <item name="android:background">@drawable/bg_launch</item> -->
    </style>

    <style name="top_menu">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:drawablePadding">0.0dip</item>
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="bottom">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:background">@drawable/base_action_bar_bg</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:button">@null</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:contentDescription">@null</item>
    </style>

    <style name="MyRadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:drawableLeft">@drawable/radiobutton_selector</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:textColor">@color/color_black</item>
    </style>

    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:drawableLeft">@drawable/multi_check_btn</item>
        <item name="android:button">@null</item>
        <item name="android:drawablePadding">5dp</item>
        <item name="android:paddingLeft">5dp</item>
    </style>

    <style name="edittext_style" parent="@android:style/Widget.EditText">
        <item name="android:background">@drawable/edittext_bg</item>
    </style>

    <style name="text_black_18" parent="@android:style/TextAppearance">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">18sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="text_black_20" parent="@android:style/TextAppearance">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">20sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="text_black_22" parent="@android:style/TextAppearance">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">22sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="text_black_24" parent="@android:style/TextAppearance">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">24sp</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="text_black_26" parent="@android:style/TextAppearance">
        <item name="android:textColor">#565656</item>
        <item name="android:textSize">26sp</item>
        <item name="android:singleLine">true</item>
    </style>

</resources>