<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/ct_traffic__color_white"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="10dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ct_traffic__name_"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="3"
            android:textColor="@color/ct_traffic__color_black"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ct_traffic__type_"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ct_traffic__address_"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="3"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/phone_layout"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/ct_traffic__tel_"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/tel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:maxLines="3"
            android:textColor="@color/ct_traffic__text_color_gray"
            android:textSize="18sp" />
    </LinearLayout>

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="10dp" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:orientation="vertical" >

            <LinearLayout
                android:id="@+id/name_error_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:addStatesFromChildren="true"
                android:background="@drawable/ct_traffic__setting_item_top"
                android:gravity="center" >

                <TextView
                    android:id="@+id/name_error_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text="@string/ct_traffic__feedback_name_error"
                    android:textColor="@color/ct_traffic__name_textview_color"
                    android:textSize="18sp" >
                </TextView>

                <CheckBox
                    android:id="@+id/name_error_checkbox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/address_error_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:addStatesFromChildren="true"
                android:background="@drawable/ct_traffic__setting_item_center"
                android:gravity="center" >

                <TextView
                    android:id="@+id/address_error_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text="@string/ct_traffic__feedback_address_error"
                    android:textColor="@color/ct_traffic__name_textview_color"
                    android:textSize="18sp" >
                </TextView>

                <CheckBox
                    android:id="@+id/address_error_checkbox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/tel_error_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:addStatesFromChildren="true"
                android:background="@drawable/ct_traffic__setting_item_center"
                android:gravity="center" >

                <TextView
                    android:id="@+id/tel_error_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text="@string/ct_traffic__feedback_tel_error"
                    android:textColor="@color/ct_traffic__name_textview_color"
                    android:textSize="18sp" >
                </TextView>

                <CheckBox
                    android:id="@+id/tel_error_checkbox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/place_error_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:addStatesFromChildren="true"
                android:background="@drawable/ct_traffic__setting_item_center"
                android:gravity="center" >

                <TextView
                    android:id="@+id/place_error_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text="@string/ct_traffic__feedback_place_error"
                    android:textColor="@color/ct_traffic__name_textview_color"
                    android:textSize="18sp" >
                </TextView>

                <CheckBox
                    android:id="@+id/place_error_checkbox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/other_error_layout"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:addStatesFromChildren="true"
                android:background="@drawable/ct_traffic__setting_item_bottom"
                android:gravity="center" >

                <TextView
                    android:id="@+id/other_error_text"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_weight="1"
                    android:text="@string/ct_traffic__feedback_other_error"
                    android:textColor="@color/ct_traffic__name_textview_color"
                    android:textSize="18sp" >
                </TextView>

                <CheckBox
                    android:id="@+id/other_error_checkbox"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="false"
                    android:focusable="false"
                    android:focusableInTouchMode="false" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/ct_traffic__description"
                    android:textColor="@color/ct_traffic__text_color_gray"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/description_edit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/ct_traffic__input_description"
                    android:maxLines="3"
                    android:textColor="@color/ct_traffic__color_black"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/ct_traffic__feedback_contact"
                    android:textColor="@color/ct_traffic__text_color_gray"
                    android:textSize="18sp" />

                <EditText
                    android:id="@+id/contact_edit"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/ct_traffic__input_feedback_contact"
                    android:maxLines="3"
                    android:textColor="@color/ct_traffic__color_black"
                    android:textSize="18sp" />
            </LinearLayout>

            <Button
                android:id="@+id/submit"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:text="@string/ct_traffic__submit"
                android:textColor="@color/ct_traffic__name_textview_color"
                android:textSize="20sp" />
        </LinearLayout>
    </ScrollView>

</LinearLayout>